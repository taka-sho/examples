{
  "name": "@uncovertruth/examples",
  "description": "USERDIVE examples",
  "version": "0.0.1",
  "author": "UNCOVER TRUTH Inc",
  "bugs": {
    "url": "https://github.com/uncovertruth/examples/pulls"
  },
  "dependencies": {
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@angular/upgrade": "^4.4.6",
    "@uirouter/angularjs": "^1.0.10",
    "angular": "^1.6.6",
    "core-js": "^2.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "systemjs": "^0.20.19",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/angular": "^1.6.36",
    "@types/angular-ui-router": "^1.1.40",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.47",
    "@types/power-assert": "^1.4.29",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-router": "^4.0.16",
    "@types/webdriverio": "^4.8.6",
    "@uncovertruth/eslint-config": "^3.1.0",
    "@uncovertruth/tslint-config": "^1.2.0",
    "concurrently": "^3.5.0",
    "eslint": "^4.10.0",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "protractor": "^5.2.0",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.5.0",
    "pug-lint-config-uncovertruth": "^0.0.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^3.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "homepage": "https://github.com/uncovertruth/examples#readme",
  "keywords": [
    "examples",
    "userdive"
  ],
  "lib": [
    "src"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --single-quote --parser=typescript",
      "tslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier-eslint --write --single-quote",
      "eslint --cache",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uncovertruth/examples.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:pug": "pug src/templates --out dist --silent",
    "build:ts": "webpack",
    "deploy": "gh-pages -d dist",
    "lint": "run-p lint:*",
    "lint:js": "eslint . --cache",
    "lint:pug": "pug-lint src/templates/",
    "lint:ts": "run-p lint:ts:*",
    "lint:ts:src": "tslint \"src/app/**/*.ts\"",
    "lint:ts:test": "tslint \"test/**/*.ts\"",
    "prebuild": "rimraf dist tags/",
    "precommit": "lint-staged",
    "prestart": "npm run build",
    "pretest": "rimraf tmp && tsc test/*.ts --outDir tmp/",
    "prew:e2e": "webdriver-manager update --standalone",
    "start": "concurrently --kill-others 'npm run w:e2e' 'npm run w:dev'",
    "test": "run-p test:*",
    "test:e2e": "protractor protractor.conf.js",
    "test:lint": "npm run lint",
    "w:dev": "webpack-dev-server --content-base dist/",
    "w:e2e": "webdriver-manager start"
  }
}
