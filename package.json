    "url": "https://github.com/uncovertruth/examples/pulls"
  },
  "dependencies": {
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@angular/upgrade": "^4.2.3",
    "@uirouter/angularjs": "^1.0.4",
    "angular": "^1.6.4",
    "core-js": "^2.4.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "reflect-metadata": "^0.1.10",
    "riot": "^3.5.1",
    "riot-route": "^3.1.1",
    "rxjs": "^5.4.1",
    "systemjs": "^0.20.14",
    "vue": "^2.3.3",
    "vue-router": "^2.6.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@types/angular": "^1.6.20",
    "@types/angular-ui-router": "^1.1.37",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.32",
    "@types/power-assert": "^1.4.29",
    "@types/react": "^15.0.30",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.9",
    "@types/riot": "^2.6.2",
    "@types/webdriverio": "^4.7.1",
    "@uncovertruth/eslint-config": "^3.0.1",
    "@uncovertruth/tslint-config": "^1.1.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "ghpages": "^0.0.10",
    "husky": "^0.14.0",
    "lint-staged": "^3.6.1",
    "mocha": "^3.4.1",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.4",
    "prettier": "^1.4.4",
    "protractor": "^5.1.2",
    "pug": "^2.0.0-rc.1",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.4.0",
    "pug-lint-config-uncovertruth": "^0.0.2",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.4",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.5.0"
  },
  "homepage": "https://github.com/uncovertruth/examples#readme",
  "keywords": [
    "examples",
    "userdive"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --single-quote --parser=typescript",
      "tslint",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uncovertruth/examples.git"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:pug": "pug src/templates --out dist --silent",
    "build:ts": "webpack",
    "deploy": "ghpages --path dist",
    "lint": "npm-run-all -p lint:*",
    "lint:js": "eslint . --cache",
    "lint:pug": "pug-lint src/templates/",
    "lint:ts": "npm-run-all -p lint:ts:*",
    "lint:ts:src": "tslint \"src/app/**/*.ts\"",
    "lint:ts:test": "tslint \"test/**/*.ts\"",
    "prebuild": "rimraf dist tags/",
    "prebuild:ts": "riot --config riot.config",
    "precommit": "lint-staged",
    "prestart": "npm run build",
    "pretest": "tsc test/*.ts --outDir tmp/",
    "prew:e2e": "webdriver-manager update --standalone",
    "start": "concurrently --kill-others 'npm run w:e2e' 'npm run w:dev'",
    "test": "npm-run-all -p test:*",
    "test:e2e": "protractor protractor.conf.js",
    "test:lint": "npm run lint",
    "w:dev": "webpack-dev-server --content-base dist/",
    "w:e2e": "webdriver-manager start"
  }
}
