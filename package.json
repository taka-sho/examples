{
  "name": "@uncovertruth/examples",
  "description": "USERDIVE examples",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uncovertruth/examples.git"
  },
  "keywords": [],
  "author": "UNCOVER TRUTH Inc",
  "bugs": {
    "url": "https://github.com/uncovertruth/examples/issues"
  },
  "homepage": "https://github.com/uncovertruth/examples#readme",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:pug": "pug src/templates --out dist",
    "build:ts": "webpack",
    "lint": "npm-run-all -p lint:*",
    "lint:js": "eslint .",
    "lint:pug": "pug-lint src/templates/",
    "lint:ts": "npm-run-all -p lint:ts:*",
    "lint:ts:src": "tslint \"src/app/**/*.ts\"",
    "lint:ts:test": "tslint \"test/**/*.ts\"",
    "postinstall": "typings install",
    "postpublish": "greenkeeper-postpublish",
    "prebuild": "rm -rf dist",
    "prestart": "npm run build",
    "pretest": "npm run lint",
    "start:selenium": "selenium-standalone start",
    "test": "npm run build",
    "test:wdio": "wdio wdio.conf.js"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0"
  },
  "devDependencies": {
    "@uncovertruth/eslint-config": "^0.0.1",
    "@uncovertruth/tslint-config": "^0.0.1",
    "eslint": "^3.1.1",
    "greenkeeper-postpublish": "^1.0.0",
    "npm-run-all": "^2.3.0",
    "pug": "^2.0.0-beta4",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.3.0",
    "pug-lint-config-uncovertruth": "^0.0.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "typings": "^1.3.2",
    "webpack": "^1.13.1",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "selenium-standalone": "^5.4.0",
    "selenium-test-runner": "^0.3.1",
    "wdio-mocha-framework": "^0.3.5",
    "webdriverio": "^4.1.1"
  },
  "private": true
}
