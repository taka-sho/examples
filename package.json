{
  "name": "@uncovertruth/examples",
  "description": "USERDIVE examples",
  "version": "0.0.1",
  "author": "UNCOVER TRUTH Inc",
  "bugs": {
    "url": "https://github.com/uncovertruth/examples/pulls"
  },
  "dependencies": {
    "angular": "^1.6.0",
    "angular-ui-router": "^0.3.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "riot": "^3.0.4",
    "riot-route": "^3.0.2",
    "vue": "^2.1.7",
    "vue-router": "^2.1.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.52",
    "@types/power-assert": "^1.4.29",
    "@types/riot": "^2.6.1",
    "@types/webdriverio": "^4.0.32",
    "@types/vue": "^1.0.31",
    "@types/vue-router": "^0.7.27",
    "@uncovertruth/eslint-config": "^1.0.2",
    "@uncovertruth/tslint-config": "^0.0.2",
    "concurrently": "^3.1.0",
    "eslint": "^3.12.1",
    "ghpages": "^0.0.8",
    "greenkeeper-postpublish": "^1.0.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "power-assert": "^1.4.2",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.3.0",
    "pug-lint-config-uncovertruth": "^0.0.1",
    "rimraf": "^2.5.4",
    "riot-router": "^0.8.1",
    "selenium-standalone": "^5.9.0",
    "ts-loader": "^1.3.2",
    "ts-node": "^1.7.2",
    "tslint": "^4.0.2",
    "typescript": "^2.1.4",
    "typings": "^2.0.0",
    "wdio-mocha-framework": "^0.5.7",
    "webdriverio": "^4.5.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "homepage": "https://github.com/uncovertruth/examples#readme",
  "keywords": [
    "examples",
    "userdive"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uncovertruth/examples.git"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:pug": "pug src/templates --out dist",
    "build:ts": "NODE_ENV=production webpack",
    "deploy": "ghpages --path dist",
    "install:selenium": "selenium-standalone install",
    "install:typings": "typings install",
    "lint": "npm-run-all -p lint:*",
    "lint:js": "eslint .",
    "lint:pug": "pug-lint src/templates/",
    "lint:ts": "npm-run-all -p lint:ts:*",
    "lint:ts:src": "tslint \"src/app/**/*.ts\"",
    "lint:ts:test": "tslint \"test/**/*.ts\"",
    "postinstall": "npm-run-all -p install:*",
    "postpublish": "greenkeeper-postpublish",
    "prebuild": "rimraf dist tags/",
    "prebuild:ts": "riot --config riot.config",
    "prestart": "npm run build:pug",
    "start": "concurrently --kill-others  \"npm run start:dev\" \"npm run start:selenium\"",
    "start:dev": "webpack-dev-server --content-base dist/",
    "start:selenium": "selenium-standalone start",
    "test": "npm-run-all -p test:*",
    "test:build": "npm run build",
    "test:lint": "npm run lint",
    "test:wdio": "wdio wdio.conf.js"
  }
}
